            <string>///Platform physics

//Die code
if(hp &lt;=0){
    scr_death();
}


    //Moving left and right
    //Checks the only the right key is pressed
    if(keyboard_check(vk_right) &amp;&amp; place_free(x+3,y)&amp;&amp; !keyboard_check(vk_left)){
        //If moving right then move x position +3
        sprite_index = spr_walk;
        image_speed = 0.3;
        image_xscale =1;
        if(place_free(x,y+1)){
            //Check if in the air
            if(place_free(x+3,y+vspeed)){
                sprite_index = spr_jumping;
                image_xscale =1;
                x+=3;
            }
        }else{
        sprite_index = spr_walk;
        image_speed = 0.3;
        image_xscale =1;
        x+=3;
        }
     //Checks the only the left key is pressed   
    }else if(keyboard_check(vk_left) &amp;&amp; place_free(x-3,y)&amp;&amp; !keyboard_check(vk_right)){
        //If moving left then move x position -4
        sprite_index = spr_walk;
        image_speed = 0.3;
        image_xscale = -1;
        if(place_free(x,y+1)){
            //Check if in the air
            if(place_free(x-3,y+vspeed)){
                sprite_index = spr_jumping;
                image_xscale =-1;
                x-=3;
            }
        }else{
        sprite_index = spr_walk;
        image_speed = 0.3;
        image_xscale = -1;
        x-=3;
        }
    }else{
      sprite_index = spr_idle;  
    }
    
    //Check if anything under player
    if(place_free(x,y+1)){
        gravity = .75;
        //Sets a maximum fall speed
        if(vspeed &gt;=10){
            vspeed = 7;
        }
    //If you are on the ground
    }else{
        gravity = 0;
        
        //Can jump if on ground
        if(keyboard_check_pressed(vk_up)){
            sprite_index = spr_idle;
            vspeed = -12;
        }
    }

</string>
